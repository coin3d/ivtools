# Check the CVS repository setup

sim_ac_do_cvs_update=false
sim_ac_cvs_changed=false
sim_ac_cvs_problem=false
sim_ac_cvs_save_builddir=`pwd`
# Check whether --enable-cvs-auto-update or --disable-cvs-auto-update was given.
if test "${enable_cvs_auto_update+set}" = set; then
  enableval="$enable_cvs_auto_update"
  case "$enableval" in
  yes) sim_ac_do_cvs_update=true ;;
  no)  sim_ac_do_cvs_update=false ;;
  *)   { { echo "$as_me:998: error: \"$enableval\" given to --enable-cvs-update" >&5
echo "$as_me: error: \"$enableval\" given to --enable-cvs-update" >&2;}
   { (exit 1); exit 1; }; } ;;
  esac
